! XTerm resources
!
! Remember to run `xrdb < .Xresources` after changing anything.

!--------------------------!
!           URXVT          !
!--------------------------!

! URxvt config
URxvt.font: xft:FiraCode\ Nerd\ Font Mono:size=16, xft:Font\ Awesome\ 5\ Free\ Solid:size=18,\
xft:Material\ Design\ Icons:size=16, xft:PowerLineSymbols:pixelsize=12, xft:Source\ Code\ Pro-9:size=16,\
xft:Noto\ Emoji-9:size=16
URxvt.scrollTtyOutput: False
URxvt.scrollWithBuffer: True
URxvt.scrollTtyKeypress: True
URxvt.background: #000000
URxvt.foreground: #eeeeee
URxvt.jumpScroll: True
URxvt.scrollBar: False
URxvt.cursorBlink: True
URxvt.letterSpace: 1
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.urlLauncher: brave
URxvt.underlineURLs: true
URxvt.urlButton: 1
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.termName: rxvt-unicode-256color
URxvt.pointerBlank: true

! perl cmds
URxvt.perl-lib: /usr/lib/urxvt/perl/
URxvt.perl-ext-common: default,matcher

